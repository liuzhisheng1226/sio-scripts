# Random 8KB IOPS: 4 workers and each with queue depth 32.

[global]
filename=/dev/nvme0n1
ioengine=libaio

direct=1
buffered=0
invalidate=1

blocksize=8K
iodepth=32

randrepeat=0
norandommap

thread
numjobs=4

time_based
ramp_time=30
runtime=180

group_reporting

[rr8k-iops]
readwrite=randread

[rw8k-iops]
stonewall
readwrite=randwrite
