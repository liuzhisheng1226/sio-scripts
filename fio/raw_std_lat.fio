# Random latency: 4KB of transfer size and with queue depth 1.

[global]
filename=/dev/nvme0n1
ioengine=libaio

direct=1
buffered=0
invalidate=1

blocksize=4K
iodepth=1

randrepeat=0
norandommap

time_based
ramp_time=30
runtime=180

group_reporting

[rr4k-lat]
readwrite=randread

[rw4k-lat]
stonewall
readwrite=randwrite
